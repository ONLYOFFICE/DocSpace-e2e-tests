{
  "name": "testing-docspace-playwright",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node scripts/playwright-test-runner.js",
    "testUI": "npx playwright test --ui",
    "test:api": "npx playwright test --project=api-tests",
    "test:e2e": "npx playwright test --project=chromium --project=firefox",
    "test:all": "(npm run test:api || true) && npm run test:e2e",
    "docker:build": "docker-compose build",
    "docker:start": "docker-compose run --rm tests-local npx playwright test",
    "docker:start:screenshots": "docker-compose run --rm tests-local npx playwright test --update-snapshots",
    "lint": "eslint \"./src/**/*.{ts,js}\"",
    "lint:fix": "eslint \"./src/**/*.{ts,js}\" --fix",
    "tsc": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.49.1",
    "@reportportal/agent-js-playwright": "^5.1.11",
    "@types/imapflow": "^1.0.22",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lefthook": "^1.12.4",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.5.3",
    "@onlyoffice/docspace-api-sdk": "^3.5.0",
    "docx": "^9.2.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "imapflow": "^1.0.200",
    "loglevel": "^1.9.2",
    "pdfkit": "^0.16.0",
    "pptxgenjs": "^3.12.0",
    "xlsx": "^0.18.5"
  }
}
